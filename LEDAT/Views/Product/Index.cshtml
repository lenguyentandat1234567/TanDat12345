@model List<QLTV.Models.Product>
@{
    ViewBag.Title = "Product";
    Layout = "~/Views/Share/_LayoutPage1.cshtml";
}

<h2>Product</h2>
<table class="table">
    <tr>
        <th>Product Id</th>
        <th>Product Name</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.ProductId</td>
            <td>@item.ProductName</td>
        </tr>
    }
</table>
